package com.techlabs.opencloseprincipal.refactor;

public class CalulateRate {
	
	public double findRate(FixedDeposit fixedDeposit)
	{
		if(fixedDeposit.festival.toString().equals("holi"))
		{
		  return 8.0;
		}else if(fixedDeposit.festival.toString().equals("newyear"))
		{
			return 9.0;
		}else if(fixedDeposit.festival.toString().equals("normal")){
			return 7.0;
		}else
			{
			return 0.0;
			}
	}
}
