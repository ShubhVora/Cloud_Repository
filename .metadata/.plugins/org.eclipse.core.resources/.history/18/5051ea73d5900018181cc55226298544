package com.techlabs.bankapp;

import java.util.Date;

public class Check {

	public boolean joiningIsPossible(Date birth, Date join) {
		if (birth.compareTo(join) > 0) {
			return true;
		} else {
			return false;
		}
	}

	public boolean workingStatus(int workStatus) {
		if (workStatus == 1) {
			return true;
		} else {
			return false;
		}
	}

}
