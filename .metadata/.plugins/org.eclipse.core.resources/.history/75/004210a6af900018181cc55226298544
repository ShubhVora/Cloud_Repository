package com.techlabs.Shopping_Cart;

import java.util.ArrayList;
import java.util.HashMap;

public class Order {

	private static int orderId = 0;
	private String date;
	private HashMap<Integer, LineItem> arrayListOfLineItem;

	public Order(LineItem line) {
		orderId++;
		this.arrayListOfLineItem.put(this.orderId, line);
	}

	public int getOrderId()
	{
		return arrayListOfLineItem
	}

}
