package com.techlabs.shoppingcart;

import java.util.ArrayList;

public class Customer {
	private String name, address;
	private int contact;

	public Customer(String name, String address, int contact) {
		this.name = name;
		this.address = address;
		this.contact = contact;
	}

	public double placeCustomerOrder(ArrayList<LineItem> cartProduct) {
		for(LineItem lineItem : cartProduct)
		double totalCost = getTotalCost();
		return totalCost;

	}

	public void cancelCustomerOrder() {
		this.customerOrder = null;
	}

	

}
