package com.techlabs.bankapptest;

import java.util.ArrayList;
import java.util.Date;

import com.techlabs.bankapp.Employee;
import com.techlabs.bankapp.LoadFile;
import com.techlabs.bankapp.Specialize;

public class LoadFileTest {

	public static void main(String args[]) {

		Date date = new Date();
		LoadFile load = new LoadFile();
		ArrayList<Employee> list = new ArrayList<Employee>();
		ArrayList<Employee> newList = new ArrayList<Employee>();
		Employee employee12 = new Employee();
		employee12.setName("Shubham");
		employee12.setCode(1);
		employee12.setSpecialize(Specialize.Equity);
		employee12.setBirthDate(date);
		employee12.setJoiningDate(date);
		employee12.setCtc(10000.0);
		employee12.setWorkingStatus(true);
		list.add(employee12);
		load.write(list);
		newList = load.read();

		for (Employee employee : newList)
			System.out.println(employee.getName() + employee.getCode() + employee.getSpecialize() + employee.getCtc());

	}

}



package com.techlabs.SerializeApp;

import java.io.*;

public class Database{
	
	void serialize(Student s1) throws Exception
	{
	
		FileOutputStream student = new FileOutputStream("Student.ser"); 
		ObjectOutputStream out=new ObjectOutputStream(student);
		out.writeObject(s1); 
		out.close();
		
	}
	Student deserialize() throws Exception
	{
		Student s=null;
		  ObjectInputStream in=new ObjectInputStream(new FileInputStream("Student.ser"));  
		  s=(Student)in.readObject();
		  in.close();

		 return s;
	}

}
